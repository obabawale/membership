                                        <t t-if="plan.get('name').lower()=='standard'">
                                            <t t-set="pclass" t-value="'plan-standard'"/>
                                            <t t-set="icon" t-value="'icon-family.png'"/>
                                        </t>

                                        <t t-if="plan.get('name').lower()=='premium'">
                                            <t t-set="pclass" t-value="'plan-premium'"/>
                                            <t t-set="icon" t-value="'icon-family.png'"/>
                                        </t>

                                        <t t-if="plan.get('name').lower()=='international'">
                                            <t t-set="pclass" t-value="'plan-international'"/>
                                            <t t-set="icon" t-value="'icon-family.png'"/>
                                        </t>

                                        <div t-attf-class="plan-teaser {{pclass}}" style="background-color: #55acba;">
                                            <img class="plan-image"
                                                t-attf-src="/eha_website/static/src/img/{{icon}}"/>
                                                <h3 class="plan-title">
                                                    <span t-esc="plan.get('name')"/>
                                                </h3>
                                            
                                            <t>
                                                <div class="plan-details">
                                                    <span>Up to
                                                        <span t-esc="plan.get('category_discount')"/>
                                                        % discount
                                                    </span>
            
                                                </div>
                                                <a href="/family-standard-subscription"
                                                    class="btn-outline-invert" 
                                                    t-if="plan.get('name').lower()=='standard'">Customise Plan
                                                </a>
                                                <a href="/family-premium-subscription"
                                                    class="btn-outline-invert" 
                                                    t-if="plan.get('name').lower()=='premium'">Customise Plan
                                                </a>
                                                <a href="/family-premium-international-subscription"
                                                    class="btn-outline-invert" 
                                                    t-if="plan.get('name').lower()=='international'">Customise Plan
                                                </a>
                                            </t>
                                        </div>
                                    <!-- </t> -->
                                </div>
                            </t>
                        </div>
                    </div>
                </t>
            </t>
        </template>
        <!--membership subscription-->
        <template id="single_subscription_detail">
            <t t-call="website.layout">
                <div class="tab-content">
                    <t t-foreach="products" t-as="prod">
                        <t t-if=" 'standard' in prod.name.lower()">
                            <t t-set="planClass" t-value="'plan-standard'"/>
                        </t>
            
                        <t t-if=" 'premium' in prod.name.lower()">
                            <t t-set="planClass" t-value="'plan-premium'"/>
                        </t>
            
                        <t t-if=" 'international' in prod.name.lower()">
                            <t t-set="planClass" t-value="'plan-international'"/>
                        </t>
                        <div itemscope="itemscope" itemtype="http://schema.org/Product" id="wrap" class="js_sale">
                            <section class="container" id="product_detail" >
                                <div id="wrap" class="oe_structure oe_empty"/>
                                <div t-attf-class="container {{planClass}}">
                                    <div class="row mt-5">
                                        <ul class="breadcrumb">
                                            <!-- <li class="breadcrumb-item">
                                                <a t-att-href="keep(category=0)">Products</a>
                                            </li>
                                            <li t-if="category" class="breadcrumb-item">
                                                <a t-att-href="keep('/shop/category/%s' % slug(category), category=0)"
                                                   t-field="category.name"/>
                                            </li> -->
                                            <li class="breadcrumb-item active">
                                                <span t-field="prod.name"/>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="row mt-2">
                                        <div class="col-md-4">
                                            <div class="product-img">
                                                <t t-if=" 'youth' in prod.name.lower()">
                                                    <img src="/eha_website/static/src/img/icon-youth.png"/>
                                                </t>
                                                <t t-if=" 'adult' in prod.name.lower()">
                                                    <img src="/eha_website/static/src/img/icon-adult.png"/>
                                                </t>
                                                <t t-if=" 'senior' in prod.name.lower()">
                                                    <img src="/eha_website/static/src/img/icon-senior.png"/>
                                                </t>
                                                <!-- <t t-if=" 'family' in product.name.lower() ">
                                                    <img src="/eha_website/static/src/img/icon-family.png"/>
                                                </t> -->
                                            </div>
                                        </div>
                                        <div class="col-md-8" id="product_details">
                                            <h3 class="plan-title" itemprop="name" t-field="prod.name"/>
                                            <t>
                                                <t>
                                                    <!-- <h4 class="plan-subtitle">
                                                        <span t-esc="prod.name"/>
                                                    </h4> -->
                                                    <h4>
                                                        ₦<span t-esc="prod.list_price"/>/ Year
                                                    </h4>
                                                </t>
                                            </t>
                                            <span itemprop="url" style="display:none;"
                                                t-esc="'%s eha_membership/subscribe/%s' % (request.httprequest.url_root, prod.name)"/>
                                            <form>
                                                <div class="js_product js_main_product">
                                                    <button role="button" id="add_to_cart"
                                                    class="btn btn-primary btn-lg btn-round mt8 js_check_product a-submit"
                                                    href="#">
                                                        Subscribe
                                                    </button>

                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    
                    </t>
                </div>
            </t>
        </template>
    </data>
</odoo>       
            
                
                