        <template id="select_membership_category" name="Select Membership Category">
            <t t-name="website.membership_category_page" name="EHA Membership Website Page">
                <t t-call="website.layout">
                    <t t-set="title">Membership Category</t>
                    <div id="wrap" class="oe_structure oe_empty">
                        <section class="s_cover parallax page" data-scroll-background-ratio="0"
                                style="background-image: none;">
                            <span class="s_parallax_bg oe_img_bg oe_custom_bg"
                                style="background-image: url(&quot;/eha_website/static/src/img/bg-plans.jpg&quot;); background-position: 48.83% 30.35%;"/>
                            <div class="container">
                                <div class="row s_nb_column_fixed">
                                    <div class="col-lg-12 s_title pt48 pb0" data-name="Title">
                                        <h1 class="title-banner text-center">
                                            <span>Our</span>
                                            <span class="text-highlight">
                                                Membership Category
                                            </span>
                                        </h1>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <div class="tab-content">
                            <div class="tabs">
                                <a t-attf-href="/eha_membership/individual-category/{{'Individual'}}" class="tablink col-sm plan-standard"
                                        id="defaultOpen">Individual
                                </a>
                                <a t-attf-href="/eha_membership/family-category/{{'Family'}}" class="tablink col-sm plan-premium">Family
                                </a>
                                <a href="" class="tablink col-sm plan-international">
                                    Group
                                </a>
                            </div>
                        </div>
                        <!--compare plan button -->
                        <div class="container">
                            <div class="pt-4 pb-5">
                                <h2 class="section-title text-center pb-4">Not sure which plan covers your needs?</h2>
                                <p class="text-lg text-center">You want to compare all benefits of our different
                                    membership plans?<br/>We have listed all available benefits side by side.
                                </p>
                            </div>
                            <div class="text-center pb-5">
                                <a href="/shop/plans/compare" class="btn-primary btn-lg text-center mt-3 mb-6">Compare
                                    plans
                                </a>
                            </div>
                        </div> <!--//compare plan button-->
                    </div>
                </t>
            </t>
        </template>
        <template id="select_individual_membership_plan" name="Select Membership Plan">
            <t t-name="website.single_membership_plan_page">
                <t t-call="website.layout">
                    <t t-set="title">Membership Subscription</t>
                    <div id="wrap" class="oe_structure oe_empty">
                        <section class="s_cover parallax page" data-scroll-background-ratio="0"
                                style="background-image: none;">
                            <span class="s_parallax_bg oe_img_bg oe_custom_bg"
                                style="background-image: url(&quot;/eha_website/static/src/img/bg-plans.jpg&quot;); background-position: 48.83% 30.35%;"/>
                            <div class="container">
                                <div class="row s_nb_column_fixed">
                                    <div class="col-lg-12 s_title pt48 pb0" data-name="Title">
                                        <h1 class="title-banner text-center">
                                            <span>Our</span>
                                            <span class="text-highlight">
                                                Membership Plan
                                            </span>
                                        </h1>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <h3 class="text-purple text-center mt-5 mb-4">Choose a membership plan</h3>
                        <div class="tab-content">
                            <t t-foreach="plans" t-as="plan">
                                <div class="container plans mt-5">
                                    <t t-foreach="plan.get('products')" t-as="product">
                                        <t t-if=" 'youth' in product.product_id.name.lower()">
                                            <t t-set="pclass" t-value="'plan-youth'"/>
                                            <t t-set="icon" t-value="'icon-youth.png'"/>
                                        </t>

                                        <t t-if=" 'adult' in product.product_id.name.lower()">
                                            <t t-set="pclass" t-value="'plan-adult'"/>
                                            <t t-set="icon" t-value="'icon-adult.png'"/>
                                        </t>

                                        <t t-if=" 'senior' in product.product_id.name.lower()">
                                            <t t-set="pclass" t-value="'plan-senior'"/>
                                            <t t-set="icon" t-value="'icon-senior.png'"/>
                                        </t>

                                        <div t-attf-class="plan-teaser {{pclass}}" style="background-color: #55acba;">
                                            <img class="plan-image"
                                                t-attf-src="/eha_website/static/src/img/{{icon}}"/>
                                            <h3 class="plan-title">
                                                <span t-esc="product.product_id.name"/>
                                            </h3>
                                            <h4 class="plan-subtitle">
                                                <span>Up to
                                                    <span t-esc="plan.get('category_discount')"/>
                                                    % discount
                                                </span>
                                            </h4>
                                            <h4 class="plan-subtitle"
                                                t-if="product and 'senior' not in product.product_id.name.lower()">
                                                <span t-esc="product.age_min"/>-<span t-esc="product.age_max"/> Years Old
                                            </h4>
                                            <h4 class="plan-subtitle"
                                                t-else="1">
                                                <span t-esc="product.age_min"/> +Years Old
                                            </h4>
                                          
                                            <t t-if="product.display_on_website == true">
                                                <div class="plan-details"
                                                    t-if="product and 'family' not in product.product_id.name.lower()">
                                                    ₦<span t-esc="product.product_id.list_price"/>/ Year
                                                </div>
                                                <!-- <div class="plan-details" 
                                                    t-if="product and 'family' in product.product_id.name.lower()">
                                                    <span/> Customize your plan to fit your family.
                                
                                                </div> -->
                                                <a t-attf-href="/eha_membership/subscribe/{{product.product_id.name}}"
                                                class="btn-outline-invert"
                                                t-if="product and 'family' not in product.product_id.name.lower()">Select Plan
                                                </a>
                                                <!-- <a href=""
                                                class="btn-outline-invert" t-else="1">Customise Plan
                                                </a> -->
                                            </t>
                                        </div>
                                    </t>
                                </div>
                            </t>                                
                        </div>
                    </div>
                </t>
            </t>
        </template>
